{
  "name": "working-women-advocates",
  "version": "0.0.1",
  "description": "An app for working women to connect with advocates.",
  "main": "index.js",
  "scripts": {
    "build": "check-node-version --node '>= 6.7.0' && bin/setup && webpack",
    "build-watch": "check-node-version --node '>= 6.7.0' && bin/setup && webpack -w",
    "build-sass-watch": "check-node-version --node '>= 6.7.0' && bin/setup && webpack -w & sass --watch app/sass/main.scss:public/css/main.css",
    "build-branch": "bin/build-branch.sh",
    "deploy-heroku": "bin/deploy-heroku.sh",
    "postinstall": "npm run build && node-sass app/sass/main.scss public/css/main.css",
    "start": "check-node-version --node '>= 6.7.0' && bin/setup && nodemon server/start.js",
    "seed": "bin/setup && node db/seed.js",
    "test": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --compilers js:babel-register app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "test-watch": "check-node-version --node '>= 6.7.0' && bin/setup && mocha --compilers js:babel-register --watch app/**/*.test.js app/**/*.test.jsx db/**/*.test.js server/**/*.test.js",
    "sass-watch": "sass --watch app/sass/main.scss:public/css/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Working-Women-Advocates/working-women-advocates.git"
  },
  "keywords": [
    "react",
    "redux",
    "skeleton"
  ],
  "author": "Rachel Bird",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Working-Women-Advocates/working-women-advocates/issues"
  },
  "homepage": "http://working-women-advocates.herokuapp.com/",
  "dependencies": {
    "axios": "^0.15.2",
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "chai-enzyme": "^0.5.2",
    "chalk": "^1.1.3",
    "check-node-version": "^1.1.2",
    "cookie-session": "^2.0.0-alpha.1",
    "enzyme": "^2.5.1",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "nodemon": "^1.11.0",
    "node-sass": "^4.5.2",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.10",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "pretty-error": "^2.0.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.24.6",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.1.2",
    "sass-loader": "^6.0.3",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.1",
    "volleyball": "^1.4.1"
  }
}
